{
  "name": "curso-senior-plataforma",
  "version": "0.0.0",
  "scripts": {
    "start": "node server.js --ssl true",
    "start:install": "npm install && npm start",
    "start:https": "node server.js --ssl true",
    "build": "ng build --prod",
    "build:stats": "npm build -- --stats-json && npx webpack-bundle-analyzer dist/stats.json",
    "test": "ng test --watch=false",
    "test:dev": "ng test",
    "test:coverage": "npm test && npx http-server src/coverage -o",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "format": "prettier --write . --ignore-path .gitignore && npm lint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.11",
    "@angular/cdk": "~7.3.3",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/forms": "~8.2.11",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/platform-server": "~8.2.14",
    "@angular/router": "~8.2.11",
    "@ngx-translate/core": "^12.1.2",
    "@seniorsistemas/angular-components": "12.11.3",
    "@seniorsistemas/platform-components": "4.1.1",
    "@seniorsistemas/primeng-theme": "7.1.2",
    "@seniorsistemas/senior-core": "^2.0.0",
    "@seniorsistemas/senior-platform-data": "^3.0.0",
    "angular2-hotkeys": "~2.1.5",
    "babel-polyfill": "^6.26.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "2.5.7",
    "moment": "^2.27.0",
    "ngx-moment": "^5.0.0",
    "ng2-currency-mask": "^9.0.2",
    "ngx-take-until-destroy": "^5.4.0",
    "primeng": "~8.0.3",
    "rxjs": "~6.5.4",
    "rxjs-compat": "^6.5.4",
